{
  "id": "FSM#2",
  "target_contract": "StateMachine",
  "contracts": {
    "StateMachine": {
      "address": "0x00",
      "name": "StateMachine"
    }
  },
  "actions": {
    "initiate": {"StateMachine": ["initiate"]},
    "approve": {"StateMachine": ["approve"]},
    "receive": {"StateMachine": ["receive"]},
    "finish": {"StateMachine": ["finish"]}
  },
