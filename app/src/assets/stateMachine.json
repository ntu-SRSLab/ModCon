  ],
  "transitions": [
    {
      "from": "Applied",
      "to": "Initiated",
      "label": "initiate",
      "event": "initiate",
      "action": "initiate"
    },
    {
      "from": "Initiated",
      "to": "Approved",
      "label": "approve",
      "event": "approve",
      "action": "approve"
    },
    {
      "from": "Approved",
      "to": "Received",
      "label": "receive",
      "event": "receive",
      "action": "receive"
    },
    {
      "from": "Received",
      "to": "Finished",
      "label": "finish",
      "event": "finish",
      "action": "finish"
    }
  ]
